<form>
	<input class="form-control"
	       type="search"
	       name="search"
	       placeholder="Begin Typing To Search ..."
	       id="searchInput"
	       autofocus
	       hx-get="q"
	       hx-trigger="keyup changed delay:200ms, search, load"
	       hx-target="#searchresult"
	       hx-swap="innerHTML"
	       hx-indicator=".htmx-indicator">
</form>
<div class="htmx-indicator" aria-busy="true"></div>
<div id="searchresult" />
